select	PUBLISHTIMESTAMP, LASTUPDDTTM - PUBLISHTIMESTAMP as seks --((LASTUPDDTTM - PUBLISHTIMESTAMP)*24*60*60) as seks
from	PSIBLOGHDR
where	IB_OPERATIONNAME = 'AT_MSG_EDMS'
and		PUBLISHTIMESTAMP > sysdate - 0.5
order by PUBLISHTIMESTAMP
