<h2>Edit Jobdefinition</h2>

<% form_for(@jobdefinition) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :system_name %>
    <%= f.text_field :system_name %>
  </p>
  <p>
    <%= f.label :prcsname %>
    <%= f.text_field :prcsname %>
  </p>
  <p>
    <%= f.label :short_desc %>
    <%= f.text_field :short_desc %>
  </p>
  <p>
    <%= f.label :description %>
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.label :active %>
    <%= f.check_box :active %>
  </p>

  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @jobdefinition %> |
<%= link_to 'Back', jobdefinitions_path %>